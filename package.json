{
  "name": "react-node-interview-lab",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build -w apps/server && npm run build -w apps/client",
    "dev": "concurrently \"npm:dev:client\"",
    "dev:client": "npm run dev -w apps/client",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "concurrently": "^9.0.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4"
  }
}
